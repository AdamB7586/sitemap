{
    "name": "adamb/sitemap",
    "description": "Create a sitemap for your domain, crawls all URL's except those with nofollow links and those explicitly stated to ignore to creates a sitemap",
    "type": "library",
    "require": {
        "guzzlehttp/guzzle": "^7.0",
        "kub-at/php-simple-html-dom-parser": "^1.8"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Adam Binnersley",
            "email": "abinnersley@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Sitemap\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
          "Sitemap\\Tests\\": "tests/"
        }
    }
}
